{
	"name": "js-runner",
	"publisher": "ZolarDev",
	"displayName": "jsRunner",
	"repository": {
		"type": "git",
		"url": "https://github.com/zolarSource/jsRunner.git"
	},
	"description": "Runs js with node.js in terminal",
	"version": "2.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.jsRunner"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.jsRunner",
				"title": "Js Runner: run file with node"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == javascript",
					"command": "extension.jsRunner",
					"group": "jsRunner"
				}
			]
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
